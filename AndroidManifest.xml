

<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ostermann.sapinoscope"
    android:versionCode="1"
    android:versionName="1.0" >
    
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:name=".Sapinoscope"
        android:allowBackup="true"
        android:icon="@drawable/ic_sapin"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Parcelle_activity"
            android:label="@string/title_activity_parcelle" >
        </activity>
        <activity
            android:name=".Secteur_activity"
            android:label="@string/title_activity_secteur_activity" >
        </activity>
        <activity
            android:name=".Message_alerte_activity"
            android:label="@string/title_activity_message_alerte" >
        </activity>
        <activity
            android:name=".Ajout_sapin"
            android:label="@string/title_activity_ajout_sapin" >
        </activity>
        <activity
            android:name=".Parcelle_listView"
            android:label="@string/title_activity_parcelle_list_view" >
        </activity>
    </application>

</manifest>
